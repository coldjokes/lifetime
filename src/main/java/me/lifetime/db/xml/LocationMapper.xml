<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="me.lifetime.db.mapper.LocationMapper">
	<resultMap id="Location" type="me.lifetime.entity.Location">
		<id column="location_id" property="locationId" jdbcType="INTEGER" />
		<result column="axis_id" property="axisId" jdbcType="INTEGER" />
		<result column="name" property="name" jdbcType="VARCHAR" />
		<result column="lontitude" property="lontitude" jdbcType="DECIMAL" />
		<result column="latitude" property="latitude" jdbcType="DECIMAL" />
		<result column="scale" property="scale" jdbcType="INTEGER" />
		<result column="height" property="height" jdbcType="DOUBLE" />
		<result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
	</resultMap>

	<insert id="insert" parameterType="Location">
		insert into location(axis_id, name, lontitude, latitude, create_time, scale) values(#{axisId}, #{name}, #{lontitude}, #{latitude}, #{createTime}, #{scale})
	</insert>
</mapper>